{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "parserOptions": {
      "ecmaVersion": 2015,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "classes": true,
          "defaultParams": true
      }
  },
  "rules": {
      "react/prop-types": 0,
      "no-const-assign": 1,
      "no-extra-semi": 0,
      "semi": 0,
      "no-fallthrough": 0,
      "no-empty": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-redeclare": 0,
      "no-this-before-super": 1,
      "no-undef": 1,
      "no-unreachable": 1,
      "no-use-before-define": 0,
      "constructor-super": 1,
      "curly": 0,
      "eqeqeq": 0,
      "func-names": 0,
      "valid-typeof": 1
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
]
}